n = int(input())
mas = list(map(int,input().split()))
try:
 vibor = int(input())
except ValueError:
 print ('неверный формат данных')
exit()
count = 0
for run in range(n-1):
 for i in range(n-1-run):
# print(f"сейчас сравниваем {mas[i]} с {mas[i+1]}")
  if mas[i] > mas[i+1]:
   count += 1
mas[i],mas[i+1]=mas[i+1],mas[i]
print(*mas)
print(count)

